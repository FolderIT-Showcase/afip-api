{
  "apps": [
    {
      "name": "nodefe",
      "script": "server.js",
      "watch": true,
      "ignore_watch": [
        "\.git",
        "bower_components",
        "node_modules",
        "tmp",
        "keys",
        "public"
      ],
      "merge_logs": true,
      "env": {
        "NODE_ENV": "development",
        "PORT": "80"
      },
      "env_production": {
        "NODE_ENV": "production",
        "PORT": "3001"
      }
    }
  ]
}