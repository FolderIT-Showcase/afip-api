{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "semi": 2,
        "no-console": 0,
        "no-unused-vars": 1,
        "prefer-spread": 1,
        "require-yield": 1,
        "prefer-arrow-callback": 2,
        "prefer-template": 2,
        "eqeqeq": 1,
        "no-debugger": 1
    },
    "globals": {
        "$": true,
        "angular": true
    }
}